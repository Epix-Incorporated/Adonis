				if not string.match(v.Player, "%S") then
					clone.Nameb.Text = v.Player
				else
					clone.Nameb.Text = "["..v.Player.."]: "
				end
				clone.Visible = true
				clone.Nameb.Font = "SourceSansBold"
				
				local color = v.Color or BrickColor.White()
				clone.Nameb.TextColor3 = color.Color
				
				
				tester.Text = "["..v.Player.."]: "
